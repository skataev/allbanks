<IfModule mod_rewrite.c>
    RewriteEngine on
    RewriteRule  ^$ /    [L]
    RewriteRule  (.*) /$1 [L]
</IfModule>

<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteRule ^(.*)$ index.php?_url=/$1 [QSA,L]
</IfModule>
